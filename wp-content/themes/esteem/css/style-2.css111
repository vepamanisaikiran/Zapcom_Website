/* ----------css-hover--txt-------------------*/

.hover_img:hover {top:0 }

.sol-img-1:hover > .item_description {bottom: 59px;opacity: 0.8; /* display: block; */z-index: 99999;}

.sol-img-1:hover > .poss-img-cnt {display:none;}

.sol-img-1:hover > .portfolio_zoom {top:104px; }

.sol-img-1:hover > .portfolio_link {top:104px; }

.hover_img {
overflow: hidden;
position: relative;
height: 211px;
width: 235px;
background: #343434;
border-radius: 15px;
}

.item_description {
position: absolute;
opacity: 0;
bottom: -318px;
left: 0;
right: 0;
height: 73.5%;
background: rgba(60, 60, 47, 0.66);
text-align: center;
color: #3e3e3e;
transition: all 0.2s ease-in-out;
-moz-transition: all 0.2s ease-in-out;
-webkit-transition: all 0.2s ease-in-out;
-o-transition: all 0.2s ease-in-out;
/* display: none; */
z-index: -99999;
}

.item_description > p {
margin: 20px 0 0 0;
}

span.portfolio_link {
right: 76px;
background-image: url(http://wpdemo.superbcart.com/magnum-honda/wp-content/themes/esteem/images/fb-ft_146.png);
background-position: 0 0;
z-index: 303;
transition-delay: 0.3s;
-o-transition-delay: 0.3s;
-moz-transition-delay: 0.3s;
-webkit-transition-delay: 0.3s;

width: 33px;
height: 33px;
position: absolute;
top: -104px;
}

span.portfolio_zoom {
left: 76px;
background-image: url(http://wpdemo.superbcart.com/magnum-honda/wp-content/themes/esteem/images/yout_154.png);
background-position: 0 0;
z-index: 302;
transition-delay: 0.2s;
-o-transition-delay: 0.2s;
-moz-transition-delay: 0.2s;
-webkit-transition-delay: 0.2s;

width: 33px;
height: 33px;
position: absolute;
top: -104px;
}

.over {
/* position: absolute; */
margin: 0 49px 0 0;
width: 237px;
float: right;
margin: 0 49px 25px 0;
width: 237px;
}

.item_description .mouser-cnt {
  bottom: 37%;
  position: absolute;
  width: 100%;
}
.center-img-spt {
